  "keywords": [],
  "dependencies": {
  },
  "devDependencies": {
    "webpack": "x",
    "css-loader": "x",
    "style-loader": "x",
    "stylus-loader": "x",
    "extract-text-webpack-plugin": "x",
    "stylus" : "x",
    "cssmin" : "x",
    "autoprefixer": "x",
    "autoprefixer-stylus": "x",
    "parallelshell" : "x"
  },

  "scripts" : {
    "css:stylus" : "stylus -u autoprefixer-stylus styl/app.styl -m -o css/ -w",
    "css:autoprefixer" : "autoprefixer -b 'last 2 versions, Firefox > 20, Firefox <= 20, > 1%, opera 12' css/app.css",
    "js" : "webpack-dev-server --progress --colors",
    "work" : "parallelshell 'npm run css:stylus' 'npm run js'",
    "stopserver" : "killall node"
  },

  "pre-commit": [ ],

  "jshintConfig" : {
    "asi"           : true,
    "laxbreak"      : true,
    "bitwise"       : true,
    "boss"          : false,
    "curly"         : true,
    "eqeqeq"        : true,
    "eqnull"        : false,
    "evil"          : false,
    "expr"          : false,
    "forin"         : false,
    "immed"         : true,
    "indent"        : 2,
    "latedef"       : true,
    "loopfunc"      : false,
    "noarg"         : true,
    "node"          : true,
    "regexp"        : true,
    "regexdash"     : false,
    "strict"        : false,
    "scripturl"     : true,
    "shadow"        : false,
    "supernew"      : false,
    "sub"           : true,
    "undef"         : true,
    "white"         : true
  }

}